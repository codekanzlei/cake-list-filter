{
    "name": "codekanzlei/cake-list-filter",
    "description": "List Filter Component / Helper for easy CRUD lists",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "ListFilter"
    },
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": ">=3.0",
        "cakephp/plugin-installer": "*"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
        "ListFilter\\": "src"
        }
    }
}
